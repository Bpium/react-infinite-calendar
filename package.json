    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-watch": "^6.0.1",
    "node-sass": "^4.13.0",
    "nwb": "^0.21.5",
    "nwb-sass": "^0.8.4",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass-loader": "^7.3.1"
