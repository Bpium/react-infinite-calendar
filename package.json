    "eslint-config-react-app": "^5.2.0",
    "prop-types": "^15.5.7",
    "react-tiny-virtual-list": "^2.0.0",
    "react-transition-group": "^1.1.30",
    "recompose": "^0.28.0"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/addon-options": "^5.3.13",
    "@storybook/cli": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.2.1",
    "babel-preset-react-app": "^9.1.1",
